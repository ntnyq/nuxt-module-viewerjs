{
  "name": "@nuxt-dev/viewerjs",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.16.0",
  "description": "Viewer.js integrated for Nuxt",
  "keywords": [
    "Viewer.js",
    "image",
    "nuxt",
    "nuxt-module",
    "nuxt3",
    "nuxt4",
    "viewerjs"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://nuxt-module-viewerjs.ntnyq.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/nuxt-module-viewerjs.git"
  },
  "bugs": {
    "url": "https://github.com/ntnyq/nuxt-module-viewerjs/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "deploy": "run-s dev:prepare build play:build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint",
    "play:build": "nuxi generate playground",
    "play:dev": "nuxi dev playground",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.1",
    "defu": "^6.1.4",
    "viewerjs": "^1.11.7"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.5.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.1",
    "@types/node": "^24.3.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "nuxt": "^4.1.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,svg,yml,yaml,json}": "eslint --fix"
  }
}
